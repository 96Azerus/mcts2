# .env (Пример)
# Порт, на котором будет слушать Gunicorn (Codespaces обычно сам его определяет, но лучше указать)
PORT=8080

# Параметры AI (можешь изменить)
MCTS_TIME_LIMIT_MS=5000
MCTS_RAVE_K=500
NUM_WORKERS=2 # Начни с 1 или 2 в Codespaces
ROLLOUTS_PER_LEAF=4

# Для Flask в режиме разработки (опционально, можно убрать для "продакшена" в Codespaces)
FLASK_DEBUG=1
FLASK_ENV=development
